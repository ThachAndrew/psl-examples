{
  "infer": {
    "options": {
      "admmreasoner.maxiterations": 25000
    }
  },
  "learn": {
    "options": {
      "admmreasoner.maxiterations": 25000
    }
  },
  "options": {
    "runtime.db.intids": true,
    "runtime.log.level": "DEBUG",
    "runtime.learn": true,
    "runtime.learn.method": "ContinuousRandomGridSearch",
    "runtime.learn.output.model.path": "collective-graph-identification-learned.psl",
    "search.dirichletalpha": 1.0,
    "continuousrandomgridsearch.maxlocations": 1000,
  },
  "rules": [
    # Local
    # ======================
    # Node Labeling
    "1.0: LocalEmailHasLabel(E, L) = EmailHasLabel(E, L) ^2",
    "0.1: !EmailHasLabel(E, L) ^2",
    "EmailHasLabel(E, +L) = 1.0 .",

    # Entity Resolution
    "1.0: LocalCoRef(E1, E2) = CoRef(E1, E2) ^2",
    "1.0: SimCosineBow(E1, E2) = CoRef(E1, E2) ^2",
    "1.0: SimEmailAddress03(E1, E2) -> CoRef(E1, E2) ^2",
    "1.0: SimEmailAddress05(E1, E2) -> CoRef(E1, E2) ^2",
    "0.1: !CoRef(E1, E2) ^2",

    # Link Prediction
    "1.0: LocalManages(E1, E2) = Manages(E1, E2) ^2",
    "0.1: !Manages(E1, E2) ^2",

    # Intra
    # ======================
    # Node Labeling
    "1.0: EmailHasLabel(E1, L) & SimEmailAddress03(E1, E2) -> EmailHasLabel(E2, L) ^2",

    # Entity Resolution
    "1.0: CoRef(E1, E2) & !CoRef(E2, E3) & (E1 != E2) & (E2 != E3) & (E1 != E3) -> !CoRef(E1, E3) ^2",
    "1.0: CoRef(E1, E2) & CoRef(E2, E3) & (E1 != E2) & (E2 != E3) & (E1 != E3) -> CoRef(E1, E3) ^2",

    # Link Prediction
    "1.0: Manages(E1, E2) & Manages(E1, E3) & (E1 != E2) & (E2 != E3) & (E1 != E3) -> !Manages(E2, E3) ^2",

    # Inter
    # ======================
    "1.0: CoRef(E1, E2) -> !Manages(E1, E2) ^2",
    "1.0: CoRef(E1, E2) & EmailHasLabel(E1, L) -> EmailHasLabel(E2, L) ^2",
    "1.0: Manages(E1, E2) & EmailHasLabel(E1, '0') & (E1 != E2) -> !EmailHasLabel(E2, '0') ^2",
    "1.0: Manages(E1, E2) & EmailHasLabel(E1, '1') & (E1 != E2) -> !EmailHasLabel(E2, '1') ^2",
    "1.0: Manages(E1, E2) & EmailHasLabel(E1, '2') & (E1 != E2) -> !EmailHasLabel(E2, '2') ^2",
    "1.0: Manages(E1, E2) & EmailHasLabel(E1, '3') & (E1 != E2) -> !EmailHasLabel(E2, '3') ^2",
  ],
  "predicates": {
    "EmailHasLabel/2": {
      "evaluations": [
        {
          "evaluator": "CategoricalEvaluator",
          "options": {
            "categoricalevaluator.categoryindexes": 1
          }
        }
      ],
      "observations": {
